<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="SelectedText">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/底选文</item>
    </style>

    <style name="UnSelectedText">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/底待文</item>
    </style>

    <style name="testTitle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/title</item>
    </style>

    <style name="testSummary">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/summary</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="testContent">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/content</item>
    </style>

    <style name="layout">
        <item name="android:padding">10sp</item>
    </style>
    <!-- http://www.peise.net/tools/web/ -->
    <!-- 上右下 -->
    <color name="title">#5D016D</color>
    <color name="content">#000000</color>
    <!-- 上左下 -->
    <color name="summary">#C262D3</color>
    <!-- 上左下 -->
    <color name="backgroundColor">#FFFFFF</color>
    <!-- 上左上 -->
    <!--<color name="list">#C437D3</color>-->
    <!-- 上右上 -->
    <color name="底选文">#74217D</color>
    <color name="底待文">#000000</color>
    <!-- 下 -->
    <color name="button">#DCF900</color>
</resources>
